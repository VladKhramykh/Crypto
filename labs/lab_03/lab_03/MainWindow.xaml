<Window x:Class="lab_03.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab_03"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50"/>
            <ColumnDefinition Width="245*"/>
        </Grid.ColumnDefinitions>

        <Grid Grid.Column="0">
            <StackPanel Orientation="Vertical">
                <Button Height="140" Name="nodButton" Click="nodButton_Click">НОД</Button>
                <Button Height="140" Name="simpleButton" Click="simpleButton_Click">Simple</Button>
                <Button Height="140" Name="reverseModButton" Click="reverseModButton_Click">Reverse</Button>
            </StackPanel>
        </Grid>
        <Grid Grid.Column="1">
            <Frame Name="frame" NavigationUIVisibility="Hidden"></Frame>
        </Grid>
    </Grid>
</Window>
